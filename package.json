{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node build/server/bundle.js",
    "build": "NODE_ENV=production webpack --progress --config webpack.config.server.js",
    "dev": "cross-env NODE_ENV=development concurrently -c blue,cyan,magenta \"webpack --watch --config webpack.config.server.js\" \"sleep 3 && nodemon --watch build/server build/server/bundle.js\" \"webpack-dev-server -d --hot --inline --no-info --port 3020 --config webpack.config.client.js\""
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react": "^6.24.1",
    "express": "^4.15.4",
    "http-proxy-middleware": "^0.17.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "webpack": "^3.5.5"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.5",
    "nodemon": "^1.11.0",
    "react-hot-loader": "next",
    "webpack-dev-server": "^2.7.1"
  }
}
